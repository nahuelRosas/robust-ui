{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^lint", "^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": false
    },
    "dev": {
      "persistent": false
    },
    "clean": {
      "persistent": false
    },
    "depCheck": {
      "persistent": false
    },
    "start": {
      "persistent": false
    }
  }
}
