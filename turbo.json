{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV=production"
      ]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": [],
      "env": ["NODE_ENV=test"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": false,
      "env": [
        "NODE_ENV=development"
      ]
    },
    "clean": {
      "cache": false
    },
    "depCheck": {}
  }
}
