{
  "private": false,
  "author": "Nahuel Rosas <nahuel.rosas@mi.unc.edu.ar>",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "depCheck": "turbo run depCheck",
    "build:packages": "turbo run build --filter=!./apps/*",
    "typecheck": "turbo run typecheck --filter=!./apps/*",
    "dev": "turbo run dev --no-cache --continue --filter=!./packages/*",
    "lint": "turbo run lint --filter=!./apps/*",
    "clean": "turbo run clean && rimraf dist .turbo node_modules pnpm-lock.yaml",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev",
    "changelog": "tsx scripts/changelog.ts"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@swc/core": "^1.3.85",
    "@types/node": "^20.6.2",
    "eslint": "^8.50.0",
    "eslint-config-robust": "workspace:*",
    "next": "^13.4.19",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "turbo": "^1.10.14"
  },
  "packageManager": "pnpm@7.15.0",
  "repository": "https://github.com/nahuelrosas/robust-ui",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
