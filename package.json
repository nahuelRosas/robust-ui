{
  "name": "@robust-ui/core",
  "private": false,
  "version": "0.0.0",
  "description": "Robust UI",
  "license": "MIT",
  "author": "Nahuel Rosas <nahuel.rosas@mi.unc.edu.ar>",
  "scripts": {
    "build": "pnpm run format:write && turbo run build",
    "build:packages": "pnpm run format:write && turbo run build --filter=!./apps/* ",
    "build:apps": "pnpm run format:write && turbo run build --filter=!./packages/* ",
    "start": "turbo start",
    "dev": "pnpm run format:write && turbo dev",
    "lint": "pnpm run format:write && turbo lint --filter=!./apps/*",
    "depCheck": "pnpm run format:write && turbo run depCheck",
    "clean": "turbo run clean && rimraf dist .turbo node_modules pnpm-lock.yaml",
    "clean:docs": "turbo run clean:docs",
    "format:write": "prettier --write \"**/*.{ts,tsx,md,json, html, css}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json, html, css}\" --cache",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev",
    "generateDocs": "turbo generateDocs"
  },
  "devDependencies": {
    "@changesets/changelog-github": "latest",
    "@changesets/cli": "latest",
    "@robust-ui/eslint-config": "workspace:*",
    "@robust-ui/tsconfig": "workspace:*",
    "@swc/core": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "changeset": "latest",
    "clean-package": "latest",
    "concat-md": "latest",
    "depcheck": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-turbo": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-only-warn": "latest",
    "next": "latest",
    "prettier": "latest",
    "rimraf": "latest",
    "showdown": "latest",
    "terser": "latest",
    "tsup": "latest",
    "turbo": "latest",
    "typedoc": "latest",
    "typedoc-plugin-markdown": "latest",
    "typescript": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  },
  "repository": "https://github.com/nahuelrosas/robust-ui",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
